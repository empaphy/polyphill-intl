{
    "name": "empaphy/polyphill-intl",
    "description": "A metapackage that installs a intl polyfill if needed, and removes it if redundant.",
    "version": "1.0.1",
    "type": "metapackage",
    "keywords": ["compatibility", "polyfill", "portable", "shim", "intl"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/empaphy/polyphill-intl/issues"
    },
    "require": {
        "ext-intl": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "provide": {
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-messageformatter": "*"
    },
    "minimum-stability": "dev"
}
